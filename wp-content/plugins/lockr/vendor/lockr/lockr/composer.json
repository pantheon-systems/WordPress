{
    "name": "lockr/lockr",
    "description": "Client for use with Lockr off-site key management service.",
    "type": "library",
    "authors": [
        {
            "name": "Tynor Fujimoto",
            "email": "tynor@lockr.io"
        }
    ],
    "license": "MIT",
    "require": {
        "guzzlehttp/guzzle": "^6.3.0",
        "guzzlehttp/psr7": "^1.4.2",
        "indigophp/hash-compat": "^1.1.0",
        "paragonie/random_compat": ">=2",
        "psr/http-message": "^1.0.1",
        "psr/log": "^1.0.2",
        "symfony/yaml": "~3.4.5"
    },
    "autoload": {
        "psr-4": {
            "Lockr\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3.5",
        "squizlabs/php_codesniffer": "^3.3.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Lockr\\Tests\\": "tests/"
        }
    }
}
